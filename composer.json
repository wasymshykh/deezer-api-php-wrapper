{
    "name": "wasymshykh/deezer-api-php-wrapper",
    "description": "Deezer API wrapper for PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Waseem",
            "email": "wasymshykh@gmail.com",
            "homepage": "https://iamwaseem.com"
        }
    ],
    "support": {
        "issues": "https://github.com/wasymshykh/deezer-api-php-wrapper/issues"
    },
    "keywords": [ "deezer", "api" ],
    "homepage": "https://github.com/wasymshykh/deezer-api-php-wrapper",
    "type": "library",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^9"
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "symfony/http-client": "^4.3|^4.4|^5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "platform": {
            "php": "7.3.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
